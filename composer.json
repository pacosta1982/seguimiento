{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "almasaeed2010/adminlte": "~2.4",
        "backpack/base": "^0.9.12",
        "backpack/permissionmanager": "^3.12",
        "barryvdh/laravel-snappy": "^0.4.3",
        "cmgmyr/messenger": "^2.16",
        "cornford/googlmapper": "2.*",
        "creativeorange/gravatar": "~1.0",
        "dompdf/dompdf": "^0.8.3",
        "elibyy/tcpdf-laravel": "5.7.*",
        "fideloper/proxy": "^4.0",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "intervention/image": "^2.4",
        "jeroennoten/laravel-adminlte": "^1.24",
        "kyslik/column-sortable": "5.7.*",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^7.1",
        "niklasravnsborg/laravel-pdf": "^3.0",
        "owen-it/laravel-auditing": "^8.0",
        "phpoffice/phpword": "v0.14.*",
        "silviolleite/laravelpwa": "^1.0",
        "simplesoftwareio/simple-qrcode": "~2",
        "werneckbh/laravel-qr-code": "^0.1.3"
    },
    "require-dev": {
        "backpack/generators": "^1.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "dev-master",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
